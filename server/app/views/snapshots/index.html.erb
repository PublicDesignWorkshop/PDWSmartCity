<p id="notice"><%= notice %></p>

<h1>Snapshots</h1>

<table>
  <thead>
    <tr>
      <th>Location</th>
      <th>Timestamp</th>
      <th>Timeinterval</th>
      <th>Sensors</th>
      <th>Comments</th>
      <th>Questions</th>
      <th>Answers</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @snapshots.each do |snapshot| %>
      <tr>
        <td><%= snapshot.location_id %></td>
        <td><%= snapshot.timestamp %></td>
        <td><%= snapshot.timeinterval %></td>
        <td><%= snapshot.sensors %></td>
        <td><%= snapshot.comments %></td>
        <td><%= snapshot.questions %></td>
        <td><%= snapshot.answers %></td>
        <td><%= link_to 'Show', snapshot %></td>
        <td><%= link_to 'Edit', edit_snapshot_path(snapshot) %></td>
        <td><%= link_to 'Destroy', snapshot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Snapshot', new_snapshot_path %>
